        <child link="shank_link_fr"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000.0" lower="${-limit_shank}" upper="${limit_shank}" velocity="0.5"/>
        <origin xyz="0 0 ${-thigh_len}"/>
    </joint>

