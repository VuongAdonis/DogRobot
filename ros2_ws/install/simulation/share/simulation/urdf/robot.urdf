    <!-- Define feet  -->
    <!-- feet 1 -->
    <link name="feet_link_fr">
        <visual>
            <geometry>
                <sphere radius="${feet_radius}"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="blue"/>
        </visual>
        <collision>
