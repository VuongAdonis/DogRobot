    <!-- Define the shank  -->
    <!-- shank 1 -->
    <link name="shank_link_fr">
        <visual>
            <geometry>
                <cylinder length="${shank_len}" radius="${link_radius}"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 ${-0.5*shank_len}"/>
