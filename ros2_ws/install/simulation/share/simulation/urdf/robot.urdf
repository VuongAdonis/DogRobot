            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
        </inertial>
    </link>
    <joint name="thigh_joint_fl_to_shoulder_fl" type="revolute">
        <parent link="shoulder_link_fl"/>
        <child link="thigh_link_fl"/>
